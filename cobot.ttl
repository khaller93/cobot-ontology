<http://data.ifs.tuwien.ac.at/star2/ontology> rdf:type owl:Ontology ;
                                               owl:versionIRI <http://data.ifs.tuwien.ac.at/star2/ontology/version/2.0.0> ;
                                               cc:attributionURL "http://data.ifs.tuwien.ac.at/star2/ontology"^^xsd:anyURI ;
