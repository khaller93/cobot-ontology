#################################################################
#    Object Properties
#################################################################

###  http://data.ifs.tuwien.ac.at/star2/ontology#appliesSkill
:appliesSkill rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://purl.org/co/itemContent> ;
              owl:inverseOf :isAppliedIn ;
              rdfs:domain :ProductionStep ;
              rdfs:range :Skill ;
              rdfs:comment "This production step applies the given skill."@en ;
              rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
              rdfs:label "applies skill"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#containsParameterValue
:containsParameterValue rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://purl.org/co/element> ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain :ParameterList ;
                        rdfs:range owl:Thing ;
                        owl:propertyChainAxiom ( :hasParameter
                                                 :hasParameterValue
                                               ) ;
                        rdfs:comment "This parameter list contains the given parameter value."@en ;
                        rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                        rdfs:label "contains parameter value"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#containsPart
:containsPart rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://purl.org/co/item> ;
              rdfs:domain :BOM ;
              rdfs:range :Part ;
              rdfs:comment "This 'Bill of Materials' contains the given part."@en ;
              rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
              rdfs:label "contains part"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#containsProduct
:containsProduct rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://purl.org/co/element> ;
                 owl:inverseOf :isPartOfBOM ;
                 rdfs:domain :BOM ;
                 rdfs:range :Product ;
                 owl:propertyChainAxiom ( :containsPart
                                          :isOfProduct
                                        ) ;
                 rdfs:comment "This 'Bill of Materials' contains the given product."@en ;
                 rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                 rdfs:label "contains product"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#countryOfOrigin
:countryOfOrigin rdf:type owl:ObjectProperty ;
                 rdfs:domain :ResourceType ;
                 rdfs:comment "This resource type is produced in the given country."@en ;
                 rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                 rdfs:label "country of Origin"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasBOM
:hasBOM rdf:type owl:ObjectProperty ;
        owl:inverseOf :isBOMFor ;
        rdf:type owl:InverseFunctionalProperty ;
        rdfs:domain :Product ;
        rdfs:range :BOM ;
        rdfs:comment "This product has the given 'Bill of Materials'."@en ;
        rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
        rdfs:label "has 'Bill of Materials'"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasFirstParameter
:hasFirstParameter rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasParameter ,
                                      <http://purl.org/co/firstItem> ;
                   rdfs:domain :ParameterList ;
                   rdfs:range :Parameter ;
                   rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                   rdfs:label "has first parameter"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasFirstProductionStep
:hasFirstProductionStep rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :hasProductionStep ,
                                           <http://purl.org/co/firstItem> ;
                        rdfs:domain :ProductionPlan ;
                        rdfs:range :ProductionStep ;
                        rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                        rdfs:label "has first production step"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasFirstSkillStep
:hasFirstSkillStep rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasSkillStep ,
                                      <http://purl.org/co/firstItem> ;
                   rdfs:domain :Skill ;
                   rdfs:range :SkillStep ;
                   rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                   rdfs:label "has first skill step"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasImageDepiction
:hasImageDepiction rdf:type owl:ObjectProperty ;
                   rdfs:domain :Resource ;
                   rdfs:range dctype:Image ;
                   rdfs:comment "The given image is a depiction of this resource."@en ;
                   rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                   rdfs:label "Image depiction"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasNextParameter
:hasNextParameter rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://purl.org/co/nextItem> ;
                  rdfs:domain :Parameter ;
                  rdfs:range :Parameter ;
                  rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                  rdfs:label "has next parameter"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasNextProductionStep
:hasNextProductionStep rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://purl.org/co/nextItem> ;
                       rdfs:domain :ProductionStep ;
                       rdfs:range :ProductionStep ;
                       rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                       rdfs:label "has next operation"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasNextStep
:hasNextStep rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://purl.org/co/nextItem> ;
             rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
             rdfs:label "has next step"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasParameter
:hasParameter rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://purl.org/co/item> ;
              rdfs:domain :ParameterList ;
              rdfs:range :Parameter ;
              owl:propertyChainAxiom ( :hasParameter
                                       :hasNextParameter
                                     ) ;
              rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
              rdfs:label "has parameter"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasParameterList
:hasParameterList rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :ProductionStep
                                              :SkillStep
                                            )
                              ] ;
                  rdfs:range :ParameterList ;
                  rdfs:comment "This production step has the given parameter list. An production step can only have one parameter list or none."@en ;
                  rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                  rdfs:label "has parameter list"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasParameterValue
:hasParameterValue rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://purl.org/co/itemContent> ;
                   rdfs:domain :Parameter ;
                   rdfs:range owl:Thing ;
                   rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                   rdfs:label "has parameter value"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasProductionPlan
:hasProductionPlan rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isProductionPlanOf ;
                   rdfs:domain :Product ;
                   rdfs:range :ProductionPlan ;
                   rdfs:comment "This product has the given production plan."@en ;
                   rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                   rdfs:label "has production plan"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasProductionStep
:hasProductionStep rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://purl.org/co/item> ;
                   rdfs:domain :ProductionPlan ;
                   rdfs:range :ProductionStep ;
                   owl:propertyChainAxiom ( :hasProductionStep
                                            :hasNextProductionStep
                                          ) ;
                   rdfs:comment "This production plan has the given production step."@en ;
                   rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                   rdfs:label "has production step"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasResourceOfInterest
:hasResourceOfInterest rdf:type owl:ObjectProperty ;
                       owl:inverseOf :isOfInterestForProductionPlan ;
                       rdfs:domain :ProductionPlan ;
                       rdfs:range :Resource ;
                       owl:propertyChainAxiom ( :needsSkill
                                                :isSkillOf
                                              ) ,
                                              ( :needsSkill
                                                :needsHandlingFunction
                                              ) ;
                       rdfs:comment <http://data.ifs.tuwien.ac.at/star2/ontology> ,
                                    "The given resource could be of interest for this production plan. It could enable skills required in the production plan."@en ;
                       rdfs:label "has resource of interest"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#hasSkillStep
:hasSkillStep rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://purl.org/co/item> ;
              rdfs:domain :Skill ;
              rdfs:range :SkillStep ;
              owl:propertyChainAxiom ( :hasSkillStep
                                       :hasNextStep
                                     ) ;
              rdfs:comment "This skill has the given behavioural step."@en ;
              rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
              rdfs:label "has skill step"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#implementedBy
:implementedBy rdf:type owl:ObjectProperty ;
               rdfs:range :SoftwareComponent ;
               rdfs:comment "This concrete behaviour is implemented by given software component."@en ;
               rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
               rdfs:label "implemented by"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isAppliedIn
:isAppliedIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://purl.org/co/itemContentOf> ;
             rdfs:domain :Skill ;
             rdfs:range :ProductionStep ;
             rdfs:comment "This skill is applied in the given production step."@en ;
             rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
             rdfs:label "is applied in"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isBOMFor
:isBOMFor rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :BOM ;
          rdfs:range :Product ;
          rdfs:comment "This is a 'Bill of Materials' for the given product."@en ;
          rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
          rdfs:label "is 'Bill of Materials' for"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isBasedOnDesignPattern
:isBasedOnDesignPattern rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        owl:inverseOf :isFollowedBySWC ;
                        rdfs:domain :SoftwareComponent ;
                        rdfs:comment "This software component is based on the given design pattern."@en ;
                        rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                        rdfs:label "is based on design pattern"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isCompatibleWithActuatorType
:isCompatibleWithActuatorType rdf:type owl:ObjectProperty ;
                              owl:inverseOf :isCompatibleWithRobotType ;
                              rdfs:domain :RobotType ;
                              rdfs:range :EndEffectorType ;
                              rdfs:comment "This robot is compatible with the given end effector."@en ;
                              rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                              rdfs:label "is compatible with end effector type"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isCompatibleWithRobotType
:isCompatibleWithRobotType rdf:type owl:ObjectProperty ;
                           rdfs:domain :EndEffectorType ;
                           rdfs:range :RobotType ;
                           rdfs:comment "This actuator is compatible with a given robot."@en ;
                           rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                           rdfs:label "is compatible with robot type"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isEquippedWithActuator
:isEquippedWithActuator rdf:type owl:ObjectProperty ,
                                 owl:InverseFunctionalProperty ;
                        rdfs:domain :Robot ;
                        rdfs:range :EndEffector ;
                        rdfs:comment "This robot is equipped with the given actuator."@en ;
                        rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                        rdfs:label "is equipped with"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isFollowedBySWC
:isFollowedBySWC rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :providesKnowledgeAbout ;
                 rdfs:range :SoftwareComponent ;
                 rdfs:comment "This design pattern is followed by the given software component."@en ;
                 rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                 rdfs:label "is followed by software component"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isHousing
:isHousing rdf:type owl:ObjectProperty ;
           owl:inverseOf :locatedIn ;
           rdfs:domain :ProductionPlant ;
           rdfs:range :Resource ;
           rdfs:comment "This production plant is housing the given resource."@en ;
           rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
           rdfs:label "is housing"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isHousingActuator
:isHousingActuator rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :isHousing ;
                   rdfs:domain :ProductionPlant ;
                   rdfs:range :EndEffector ;
                   rdfs:comment "This production plant is housing the given end effector."@en ;
                   rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                   rdfs:label "is housing actuator"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isHousingRobot
:isHousingRobot rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isHousing ;
                rdfs:domain :ProductionPlant ;
                rdfs:range :Robot ;
                rdfs:comment "This production plant is housing the given robot."@en ;
                rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                rdfs:label "is housing robot"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isInstanceOf
:isInstanceOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Resource ;
              rdfs:range :ResourceType ;
              rdfs:comment "This resource is an instance of the given resource type."@en ;
              rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
              rdfs:label "is instance of"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isOfHandlingFunction
:isOfHandlingFunction rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf <http://purl.org/co/itemContent> ;
                      rdfs:domain :SkillStep ;
                      rdfs:range :HandlingFunction ;
                      rdfs:comment "The given handling function is performed in this step."@en ;
                      rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                      rdfs:label "is of handling function"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isOfInterestForProductionPlan
:isOfInterestForProductionPlan rdf:type owl:ObjectProperty ;
                               rdfs:domain :Resource ;
                               rdfs:range :ProductionPlan ;
                               rdfs:comment "This resource is of interest for the given production plan. It might enable skills that are required by the given production plan."@en ;
                               rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                               rdfs:label "is of interest for production plan"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isOfProduct
:isOfProduct rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://purl.org/co/itemContent> ;
             rdfs:domain :Part ;
             rdfs:range :Product ;
             rdfs:comment "This part is of the given product."@en ;
             rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
             rdfs:label "is of product"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isPartOfBOM
:isPartOfBOM rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf <http://purl.org/co/elementOf> ;
             rdfs:domain :Product ;
             rdfs:range :BOM ;
             rdfs:comment "This product is part of the given 'Bill of Materials'."@en ;
             rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
             rdfs:label "is part of 'Bill of Materials'"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isProductionPlanOf
:isProductionPlanOf rdf:type owl:ObjectProperty ;
                    rdfs:domain :ProductionPlan ;
                    rdfs:range :Product ;
                    rdfs:comment "This production plan is concerned with the production of the given product."@en ;
                    rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                    rdfs:label "is production plan of"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isRequiredInSkill
:isRequiredInSkill rdf:type owl:ObjectProperty ;
                   owl:inverseOf :needsHandlingFunction ;
                   rdfs:domain :HandlingFunction ;
                   rdfs:range :Skill ;
                   rdfs:comment "This handling function is used in the given skill."@en ;
                   rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                   rdfs:label "is needed in skill"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isSkillOf
:isSkillOf rdf:type owl:ObjectProperty ;
           rdfs:domain :Skill ;
           rdfs:comment "The given hardware is capable of performing this skill."@en ;
           rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
           rdfs:label "is skill of"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#isUsedInProductionPlan
:isUsedInProductionPlan rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://purl.org/co/elementOf> ;
                        owl:inverseOf :needsSkill ;
                        rdfs:domain :Skill ;
                        rdfs:range :ProductionPlan ;
                        rdfs:comment "This skill is used in the given production plan."@en ;
                        rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                        rdfs:label "is used in production plan"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#locatedIn
:locatedIn rdf:type owl:ObjectProperty ;
           rdfs:domain :Resource ;
           rdfs:range :ProductionPlant ;
           rdfs:comment "A Resource is located in the given production plant."@en ;
           rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
           rdfs:label "located in"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#needsHandlingFunction
:needsHandlingFunction rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://purl.org/co/element> ;
                       rdfs:domain :Skill ;
                       rdfs:range :HandlingFunction ;
                       owl:propertyChainAxiom ( :hasSkillStep
                                                :isOfHandlingFunction
                                              ) ;
                       rdfs:comment "This skill needs the given handling function."@en ;
                       rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                       rdfs:label "needs handling function"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#needsSkill
:needsSkill rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://purl.org/co/element> ;
            rdfs:domain :ProductionPlan ;
            rdfs:range :Skill ;
            owl:propertyChainAxiom ( :hasProductionStep
                                     :appliesSkill
                                   ) ;
            rdfs:comment "This production plan needs the given skill."@en ;
            rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
            rdfs:label "needs skill"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#offers
:offers rdf:type owl:ObjectProperty ;
        rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
        rdfs:label "offers"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#offersHandlingFunction
:offersHandlingFunction rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :offers ;
                        rdfs:range :HandlingFunction ;
                        owl:propertyChainAxiom ( :isEquippedWithActuator
                                                 :offersHandlingFunction
                                               ) ,
                                               ( :isInstanceOf
                                                 :offersHandlingFunction
                                               ) ;
                        rdfs:isDefinedBy <http://data.ifs.tuwien.ac.at/star2/ontology> ;
                        rdfs:label "offers handling function"@en .


###  http://data.ifs.tuwien.ac.at/star2/ontology#offersSkill
:offersSkill rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :offers ;
             rdfs:range :Skill ;
             owl:propertyChainAxiom ( :isEquippedWithActuator
                                      :offersSkill
                                    ) ,
                                    ( :isInstanceOf
                                      :offersSkill
