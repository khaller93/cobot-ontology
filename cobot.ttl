                                               bibo:status <http://purl.org/ontology/bibo/status/draft> ;
                                               vann:preferredNamespacePrefix "cobot" ;
                                               vann:preferredNamespaceUri "http://data.ifs.tuwien.ac.at/star2/ontology#"^^xsd:anyURI .

